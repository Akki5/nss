<script language="javascript">
function loadurl(value) 
{  
try 
{
	var url;
	switch(value)
	{
	case 1:url="1.html";
				break;
	case 2:url="2.html";
				break;
	case 3:url="3.html";
				break;
	case 4:url="4.html";
				break;
	case 5:url="5.html";
				break;
	case 6:url="6.html";
				break;
	case 7:url="7.html";
				break;
	case 8:url="cells/1.html";
				break;
	case 9:url="cells/2.html";
				break;
	case 10:url="cells/3.html";
				break;
	case 11:url="cells/4.html";
				break;
	case 12:url="cells/5.html";
				break;
	case 13:url="cells/6.html";
				break;
	case 14:url="cells/7.html";
				break;
	case 15:url="cells/8.html";
				break;
	case 16:url="cells/9.html";
				break;
	case 17:url="cells/10.html";
				break;
	case 18:url="cells/11.html";
				break;
	case 19:url="cells/12.html";
				break;
	case 20:url="cells/13.html";
				break;
	case 21:url="gallery/1.html";
				break;
	case 22:url="gallery/2.html";
				break;
	case 23:url="gallery/3.html";
				break;
	case 24:url="gallery/4.html";
				break;
	case 25:url="gallery/5.html";
				break;
	case 26:url="gallery/6.html";
				break;
	case 27:url="gallery/7.html";
				break;
	case 28:url="8.html";
				break;
	}
    xmlhttp=window.XMLHttpRequest?new XMLHttpRequest():new ActiveXObject("Microsoft.XMLHTTP");
}

catch (e)  
{  
      alert("Please upgrade your browser");  // browser doesn't support ajax.
}  
  
// the xmlhttp object triggers an event everytime the status changes  
// triggered() function handles the events  
xmlhttp.onreadystatechange=triggered;
  
// open takes in the HTTP method and url.  
xmlhttp.open("GET",url);
  
// send the request. if this is a POST request we would have  
// sent post variables: send("name=aleem&gender=male)  
// Moz is fine with just send(); but  
// IE expects a value here, hence we do send(null);
	xmlhttp.send(null);
}

function triggered() 
{  
	if (xmlhttp.readyState == 4) 
	{
        document.getElementById("featured").innerHTML=xmlhttp.responseText;  
	}	
}
</script>

